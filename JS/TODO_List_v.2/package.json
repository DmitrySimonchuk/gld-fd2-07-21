  "scripts": {
    "start:prod:backend": "node ./dist/bundle-back.js",
    "start:dev:backend": "nodemon ./src/backend/index",

    "start:prod:frontend": "http-server ./dist",
    "start:dev:frontend": "webpack serve",

    "start:prod": "yarn build:front:prod && yarn build:back:prod && concurrently \"yarn start:prod:frontend\" \"yarn start:prod:backend\"",
    "start:dev": "concurrently \"yarn start:dev:frontend\" \"yarn start:dev:backend\"",
        
    "remove:dist": "rimraf ./dist",
    
    "build:front:prod": "yarn remove:dist && webpack --config ./webpack.config.js",
    "build:back:prod": "yarn webpack --config ./webpack.config-back.js",
    
    "test": "echo \"Error: no test specified\" && exit 1"
